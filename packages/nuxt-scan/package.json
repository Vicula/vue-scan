{
  "name": "nuxt-scan",
  "version": "0.0.1",
  "description": "Nuxt module for Vue Scan performance monitoring",
  "keywords": [
    "nuxt",
    "vue",
    "performance",
    "monitor",
    "profiler",
    "devtools",
    "plugin"
  ],
  "homepage": "https://github.com/Vicula/vue-scan",
  "bugs": {
    "url": "https://github.com/Vicula/vue-scan/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Vicula/vue-scan.git"
  },
  "license": "MIT",
  "author": {
    "name": "Victor Carpenter",
    "url": "https://github.com/Vicula"
  },
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "lint": "biome lint src",
    "prepare": "nuxt-module-build",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run build && npm publish",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/devtools-kit": "^2.3.0",
    "@nuxt/kit": "^3.8.0",
    "h3": "^1.15.1",
    "vue-scan": "workspace:*"
  },
  "devDependencies": {
    "@nuxt/module-builder": "^0.5.0",
    "@nuxt/schema": "^3.8.0",
    "@types/node": "^18.0.0",
    "nuxt": "^3.8.0",
    "vue": "^3.3.0"
  },
  "peerDependencies": {
    "nuxt": "^3.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
